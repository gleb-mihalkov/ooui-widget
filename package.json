{
  "name": "ooui-widget",
  "version": "1.1.1",
  "description": "",
  "main": "index.js",
  "watch": {
    "build:js": {
      "patterns": [
        "./src",
        "./index.js",
        "./test/src"
      ],
      "extensions": "js"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:js": "browserify --debug ./test/src/index.js > ./test/bundle.js",
    "build": "npm-run-all -p build:*",
    "watch:js": "npm-watch build:js",
    "watch": "npm-run-all -p watch:*",
    "server": "http-server ./test",
    "serve": "npm-run-all -p watch server"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "http-server": "^0.11.1",
    "npm-run-all": "^4.1.3",
    "npm-watch": "^0.3.0"
  },
  "dependencies": {},
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "env"
          ],
          "plugins": [
            "transform-runtime"
          ]
        }
      ]
    ]
  }
}
